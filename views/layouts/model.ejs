<div id="loginModel" class="modal fade" role="dialog">
  <div class="modal-dialog modal-dialog-centered modal-lg">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Login in Provaswito</h4>
        <button type="button" class="close" data-dismiss="modal">
          &times;
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-sm-6">
            <img
              src="/front/images/home/clients.png"
              width="80%"
              class="img-responsive loginleftmodel"
              alt=""
            />
          </div>
          <div class="col-sm-6">
            <div class="text-center">
              <img
                src="/front/images/logo.png"
                width="50%"
                class="img-responsive"
                alt=""
              />
            </div>

            <form>
              <div class="form-group">
                <label for="email">Email address</label>
                <input
                  type="email"
                  class="form-control"
                  id="email"
                  placeholder="Enter email"
                />
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Password</label>
                <input
                  type="password"
                  class="form-control"
                  id="password"
                  placeholder="Password"
                />
              </div>

              <button
                type="submit"
                class="btn btn-primary form-inline"
                style="width: 50%"
              >
                Login</button
              >&emsp;
              <button
                type="submit"
                class="btn btn-success form-inline"
                style="width: 40%"
              >
                Create Account
              </button>
            </form>
            <div class="mt-3">
              <a
                href=""
                data-toggle="modal"
                data-target="#forgetPasswordModel"
                id="forgetlink"
                >Forget Password! Reset Your Password</a
              >
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">
          Close
        </button>
      </div>
    </div>
  </div>
</div>

